<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--关联mapper类执行映射-->
<mapper namespace="com.jhomew.mapper.StartTestMapper">

    <select id="selectStartTestByNum" resultType="com.jhomew.entity.StartTest">
        select *
        from start_test
        where num = #{num}
    </select>
</mapper>